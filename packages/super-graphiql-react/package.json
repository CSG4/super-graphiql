{
  "name": "super-graphiql",
  "version": "0.0.7",
  "description": "Super-GraphiQL is a powerful yet simple GUI designed for testing your GraphQL API",
  "main": "dist/index.js",
  "files": [
    "dist",
    "super-graphiql.js",
    "super-graphiql.min.js",
    "super-graphiql.min.css",
    "README.md",
    "LICENSE"
  ],
  "browserify-shim": {
    "react": "global:React",
    "react-dom": "global:ReactDOM"
  },
  "options": {
    "mocha": "--full-trace --require resources/mocha-bootload src/**/*-test.js"
  },
  "scripts": {
    "start": "node ./example/server/server.js",
    "example": "nodemon ./example/server/server.js",
    "apollo": "nodemon ./example/server/apollo/server.js",
    "webpack": "webpack -w --config ./example/webpack.config.js --mode development",
    "webpackdev": "webpack-dev-server -w --config ./example/webpack.config.js --mode development",
    "build": "bash ./resources/build.sh",
    "check": "flow check",
    "dev": "babel-node test/server.js",
    "lint": "eslint src",
    "lint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "precommit": "npm run testonly && lint-staged",
    "_prepublish": ". ./resources/prepublish.sh",
    "preversion": ". ./resources/checkgit.sh && npm test",
    "test": "npm run lint && npm run check && npm run testonly && npm run build",
    "testonly": "babel-node ./node_modules/.bin/_mocha $npm_package_options_mocha"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --jsx-bracket-same-line--single-quote --trailing-comma-all --write",
      "git add"
    ]
  },
  "dependencies": {
    "codemirror": "^5.26.0",
    "codemirror-graphql": "^0.6.11",
    "markdown-it": "^8.4.0"
  },
  "peerDependencies": {
    "graphql": "^0.6.0 || ^0.7.0 || ^0.8.0-b || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0",
    "prop-types": ">=15.5.0",
    "react": "^15.6.0 || ^16.0.0",
    "react-dom": "^15.6.0 || ^16.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.0.0",
    "babel": "^6.23.0",
    "babel-cli": "6.24.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.22.0",
    "babel-plugin-transform-regenerator": "6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babelify": "7.3.0",
    "browserify": "^14.4.0",
    "browserify-shim": "3.8.14",
    "chai": "^4.1.2",
    "chai-subset": "1.5.0",
    "css-loader": "^0.28.11",
    "cssnano": "^4.0.0-rc.2",
    "cssnano-cli": "^1.0.5",
    "eslint": "4.4.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-react": "7.2.0",
    "expect": "^22.4.0",
    "express": "5.0.0-alpha.5",
    "express-graphql": "0.6.7",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "flow-bin": "0.52.0",
    "graphiql-subscriptions-fetcher": "0.0.2",
    "graphql": "^0.10.1",
    "graphql-server-express": "^1.3.2",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tools": "^2.23.1",
    "html-webpack-plugin": "^3.0.6",
    "husky": "^0.14.0",
    "ignore-loader": "^0.1.2",
    "jsdom": "11.1.0",
    "lint-staged": "^4.0.0",
    "mocha": "^5.0.1",
    "mongoose": "^5.0.10",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.1",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-cli": "^5.0.0",
    "postcss-loader": "^2.1.3",
    "prettier": "^1.4.4",
    "prop-types": "15.5.8",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-test-renderer": "15.6.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "stylelint": "^9.1.3",
    "subscriptions-transport-ws": "^0.9.6",
    "uglify-js": "^3.0.20",
    "uglifyify": "^4.0.2",
    "watchify": "3.9.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlafontant/super-graphiql.git"
  },
  "author": "Albert Chen, Angela Scerbo, Eduardo Maillo, Marissa Lafontant <superGraphiQLTeam@gmail.com>",
  "license": "MIT, GraphiQL",
  "keywords": [
    "graphql",
    "graphiql",
    "super-graphiql",
    "gui",
    "ide"
  ],
  "bugs": {
    "url": "https://github.com/mlafontant/super-graphiql/issues"
  },
  "homepage": "https://github.com/mlafontant/super-graphiql#readme"
}
